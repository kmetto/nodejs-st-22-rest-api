generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String  @id @default(uuid())
  login     String  @unique
  password  String
  age       Int
  isDeleted Boolean @default(false)

  @@unique([id, isDeleted])
}
